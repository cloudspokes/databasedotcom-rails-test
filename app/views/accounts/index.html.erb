<h1>Listing accounts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>City</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @accounts.each do |account| %>
  <tr>
    <td><%= account.Name %></td>
    <td><%= account.City__c %></td>
    <td><%= link_to 'Show', account_path(account) %></td>
    <td><%= link_to 'Edit', edit_account_path(account) %></td>
  </tr>
<% end %>
</table>

